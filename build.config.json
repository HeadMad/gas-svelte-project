{
  "manifest": "src/appsscript.json",
  "package": "./package.json",
  "outDir": "dist",
  "frontend": {
    "build": true,
    "src": "src/frontend",
    "minify": true
  },
  "backend": {
    "build": true,
    "src": "src/backend",
    "concatenate": true,
    "outFile": "Code.js",
    "minify": true,
    "priorityOrder": [
      "config.js",
      "utils.js",
      "main.js"
    ]
  },
  "deployment": {
    "devDeploymentId": "",
    "prodDeploymentId": ""
  }
}